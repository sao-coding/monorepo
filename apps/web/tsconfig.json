{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~/*": [
        "./public/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "@msi/tsconfig/nextjs.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "eslint.config.mjs",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts"
  ]
}
